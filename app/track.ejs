<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Price changes for <%= productName %>&nbsp;on&nbsp;<%= productSeller %></title>
    <meta name="description" content="Tracking price changes for <%= productName %> on <%= productSeller %>. Saved money #FTW">
    <meta name="viewport" content="width=device-width">
    <meta name="google-site-verification" content="Wu9IL2WJLd4pQoZSQdDKhvMqVJZkje_p1FYRIFU94J0" />
    <link rel="shortcut icon" href="http://faviconist.com/icons/d924a197b16ce0732340f3dd6987c1ef/favicon.ico" />
    <meta name="og_title" property="og:title" content="I tracked price changes for <%= productName %> on <%= productSeller %>. Saved money #FTW"/>
    <base href="https://cheapass.in/">
    <!-- build:css styles/vendor.css -->
    <!-- bower:css -->
    <!-- endbower -->
    <!-- endbuild -->
    <!-- build:css(.tmp) styles/main.css -->
    <link rel="stylesheet" href="styles/main.css">
    <!-- endbuild -->
</head>
<body>
    <!--[if lt IE 10]>
    <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <div class="container">
        <div class="header">
            <h1 class="hide">cheapass</h1>
            <a href="/">
                <img class="logo" src="images/cheapass.png" alt="Cheapass India" />
            </a>
            <p class="text-muted">Track changes in product price. Get notifications by email.</p>
        </div>

        <div class="bookmarklet-container">
            <p>
                <a class="btn btn-danger"
                href="javascript:(function(){t=(new Date()).getTime();document.body.appendChild(document.createElement('script')).src='https://cheapass.in/cdn/cheapass.js?t='+t;})();">
                Cheapass</a>
                â˜œ Drag this to your bookmark bar
                <br>
                <a href="https://cheapass.in/blog/using-the-cheapass-bookmarklet/" target="_blank">(How does it work?)</a>
            </p>
        </div>

        <div>
            <h4 class="text-center">
                <a href="<%= productURL%>"><%= productName %></a>&nbsp;
                <a href="https://twitter.com/share" class="twitter-share-button" data-text="Tracking price changes to <%= productName %> on Cheapass." data-via="cheapass_in" data-related="aakashlpin" data-count="none">Tweet</a>
                <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');</script>
            </h4>
            <div id="prettyGraphContainer" style="min-height: 400px;">
                <!-- Graph comes here -->
            </div>
        </div>

    </div>

    <!-- build:js scripts/vendor.js -->
    <!-- bower:js -->
    <script src="../bower_components/jquery/dist/jquery.js"></script>
    <script src="../bower_components/underscore/underscore.js"></script>
    <!-- endbower -->
    <!-- endbuild -->

    <!-- Google Analytics -->
    <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-52167487-1', 'cheapass.in');
    ga('send', 'pageview');

    </script>

    <script src="//cdnjs.cloudflare.com/ajax/libs/highcharts/4.0.3/highcharts.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/highcharts/4.0.3/modules/exporting.js"></script>

    <script>
        caObj = {};
        caObj.priceHistory = [
        <% for(var i=0; i<productPriceHistory.length; i++) { %>
            {
                date: "<%= productPriceHistory[i].date %>",
                price: <%= productPriceHistory[i].price %>
            },
        <% } %>
        ];
        caObj.productName = "<%= productName %>";
        caObj.productImage = "<%= productImage %>";
    </script>

    <!-- build:js({app,.tmp}) scripts/track.js -->
    <script src="scripts/track.js"></script>
    <!-- endbuild -->
</body>
</html>
