<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Cheapass | Dashboard</title>
    <meta name="description" content="Track price changes to Flipkart, Amazon, PepperFry, Snapdeal, Infibeam, FabFurnish, Jabong, Bajaao, Myntra products and get notified by email when the price changes.">
    <meta name="viewport" content="width=device-width">
    <meta name="google-site-verification" content="Wu9IL2WJLd4pQoZSQdDKhvMqVJZkje_p1FYRIFU94J0" />
    <link rel="shortcut icon" href="http://faviconist.com/icons/d924a197b16ce0732340f3dd6987c1ef/favicon.ico" />
    <meta name="og_title" property="og:title" content="Price Tracker for products on leading e-commerce companies in India. Get notified by email when the price of your favourite product changes."/>
    <meta name="og_site_name" property="og:site_name" content="Cheapass.in"/>
    <meta name="og_url" property="og:url" content="http://cheapass.in" />
    <base href="https://cheapass.in/">
    <script type="text/javascript" src="//use.typekit.net/avk0lqt.js"></script>
    <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
    <!-- build:css styles/vendor.css -->
    <!-- bower:css -->
    <!-- endbower -->
    <!-- endbuild -->
    <!-- build:css(.tmp) styles/main.css -->
    <link rel="stylesheet" href="styles/main.css">
    <!-- endbuild -->
</head>
<body>
    <!--[if lt IE 10]>
    <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <div class="container">
        <div class="header">
            <h1 class="hide">cheapass</h1>
            <img class="logo" src="images/cheapass.png" alt="Cheapass India" />
            <p class="text-muted">Track changes in product price. Get notifications by email.</p>
        </div>

        <div class="row">
            <p class="text-right">Dashboard for <strong><%= email %></strong></p>
            <div class="col-sm-12" id="track-table">
            </div>
        </div>

        <!-- build:js scripts/vendor.js -->
        <!-- bower:js -->
        <script src="../bower_components/jquery/dist/jquery.js"></script>
        <script src="../bower_components/underscore/underscore.js"></script>
        <!-- endbower -->
        <!-- endbuild -->

        <!-- Google Analytics -->
        <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-52167487-1', 'cheapass.in');
        ga('send', 'pageview');
        </script>

        <script>
        cheapassObj = {};
        cheapassObj.userId = "<%= _id %>";
        cheapassObj.userEmailId = "<%= email %>";
        </script>

        <script src="dashboard/scripts/core.js"></script>
        <script src="dashboard/scripts/main.js"></script>
    </body>
    </html>
